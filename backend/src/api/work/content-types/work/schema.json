{
  "kind": "collectionType",
  "collectionName": "works",
  "info": {
    "singularName": "work",
    "pluralName": "works",
    "displayName": "Work"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string"
    },
    "year": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "publisher": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "edition": {
      "type": "integer"
    },
    "isbn": {
      "type": "uid"
    },
    "subjects": {
      "type": "json"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "works"
    },
    "quotes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quote.quote",
      "mappedBy": "work"
    }
  }
}
